{
	"name": "@walrus/vscode-plugin",
	"displayName": "walrus",
	"description": "walrus vscode plugin",
	"version": "0.0.1",
	"publisher": "阿康",
	"repository": "https://github.com/walrus-plus/vscode-plugin-walrus",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.walrus"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.walrus",
				"title": "Walrus Common"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
